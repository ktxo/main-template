{
  "version": 1,
  "loggers": {
    "root": {
      "handlers": ["console"],
      "level": "INFO"
    },
    "ktxo.seek": {
      "propagate": false,
      "handlers": ["console", "file"],
      "level": "INFO"
    }
  },
  "formatters": {
    "basic": {
      "class": "logging.Formatter",
      "datefmt":"%Y-%m-%d %H:%M:%S",
      "format": "%(asctime)s.%(msecs)03d %(levelname)s %(thread)d %(threadName)s %(module)s - %(funcName)s: %(message)s"
    },
    "raw": {
      "class": "logging.Formatter",
      "format": "%(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "basic"
    },
    "file": {
      "class": "logging.handlers.RotatingFileHandler",
      "formatter": "basic",
      "filename": "seek.log",
      "backupCount": 10,
      "maxBytes": 10485760
    }
  }
}
